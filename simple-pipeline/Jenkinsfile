pipeline {
 agent none
 stages {
  stage('Compile') {
   agent any
   steps {
    checkout scm
    timeout(time: 2000, unit: 'SECONDS') {
        echo "hello"
    }
   }
  }
  stage('Sonarqube Test') {
   agent any
   steps {
    checkout scm
    echo env.GIT_BRANCH
    script {
     if (env.GIT_BRANCH.startsWith("origin/master")) {
      timeout(time: 2000, unit: 'SECONDS') {
       echo "master"
      }
     } else {
      echo "SonarQube Analysis Skipped"
     }
    }
   }
  }
 }
}
